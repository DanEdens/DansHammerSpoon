# Example configuration for KineticLatch.spoon
# Copy this to ~/.hammerspoon/.config or set via hs.settings

# Kinetic Latch Configuration
KINETIC_LATCH_ENABLED=true
KINETIC_LATCH_MOVE_MODIFIER=alt
KINETIC_LATCH_RESIZE_MODIFIER=alt
KINETIC_LATCH_MOVE_BUTTON=left
KINETIC_LATCH_RESIZE_BUTTON=right
KINETIC_LATCH_SENSITIVITY=1.0
KINETIC_LATCH_MIN_WIDTH=100
KINETIC_LATCH_MIN_HEIGHT=100
KINETIC_LATCH_DEBUG=false
KINETIC_LATCH_AUTO_START=true
